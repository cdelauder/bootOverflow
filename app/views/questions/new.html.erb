<%= link_to 'log out', session_path(current_user), :method => :delete %>
<br>
<%= form_for @question do |f|%>
  <%= f.label :title %>
  <%= f.text_field :title, :placeholder => 'title' %>
  <%= f.label :content %>
  <%= f.text_field :content, :placeholder => 'content' %>
  <%= f.submit 'submit' %>
<% end %>