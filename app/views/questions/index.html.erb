<%= link_to 'Post a new question', new_question_path %>

<% @questions.each do |question| %>
  <h2><%= question.title  %></h2>
  <p>created by <%= question.user.username %></p>
  <br>
  <%= question.content  %>
  <br>
  <br>
  <%= link_to 'Delete', question_path(question), :method => :delete %>

<% end %>