<%= form_tag sessions_path do %>
  <%= label_tag :username %>
  <%= text_field_tag :username, params[:username] %>
  <%= label_tag :password %>
  <%= password_field_tag :password, params[:password] %>
  <%= submit_tag 'Log In' %>
<% end %>

<%= form_tag new_users_path do %>
  <%= submit_tag 'Sign Up' %>
<% end %>